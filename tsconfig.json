{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "jsx": "preserve",
    "target": "es6"
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "node_modules"
  ],
  "files": [
    "./src/Entries/index.tsx"
  ]
}